{
  "compilerOptions": {
    "allowJs": true,
    "lib": [ "es2022", "dom", "dom.iterable" ],
    "module": "esnext",
  
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "strict": true,

    "declaration": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es2022",

    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}